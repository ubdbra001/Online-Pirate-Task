  <Routines>
    <Routine name="studyTrial">
      <CodeComponent name="feedback_code">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="r_val = []&amp;#10;l_val = []&amp;#10;&amp;#10;#create reward values&amp;#10;for t in range(0,num_trial):&amp;#10;    val = rand.randint(1,101)&amp;#10;    r_val.append(val)&amp;#10;    l_val.append(100-val)&amp;#10;&amp;#10;player_score = 0&amp;#10;pirate_level = 0&amp;#10;level_limit = 460&amp;#10;leveled_up = False&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="r_val = [];&amp;#10;l_val = [];&amp;#10;&amp;#10;&amp;#10;for (var t = 0, _pj_a = num_trial; (t &lt; _pj_a); t += 1) {&amp;#10;    var val = util.randint(1, 100);&amp;#10;    r_val.append(val);&amp;#10;    l_val.append((100 - val));&amp;#10;}&amp;#10;player_score = 0;&amp;#10;pirate_level = 0;&amp;#10;level_limit = 460;&amp;#10;leveled_up = false;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="trial_r_val = r_val[currentLoop.thisN];&amp;#10;trial_l_val = l_val[currentLoop.thisN];&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="#Assuming you have a r_val and and l_val list:&amp;#10;&amp;#10;trial_r_val = r_val[ currentLoop.thisN ]&amp;#10;trial_l_val = l_val[ currentLoop.thisN ]&amp;#10;&amp;#10;# You'll also want to add some code here to tell the program which side is correct based on prob_bg" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Both" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="if ((key_resp.keys === &quot;left&quot;)) {&amp;#10;    if (key_resp.corr) {&amp;#10;        imageVariable = (imagePath + &quot;Lgreenwin.bmp&quot;);&amp;#10;        player_score += trial_l_val;&amp;#10;    } else {&amp;#10;        imageVariable = (imagePath + &quot;Lgreenlose.bmp&quot;);&amp;#10;    }&amp;#10;} else {&amp;#10;    if ((key_resp.keys === &quot;right&quot;)) {&amp;#10;        if (key_resp.corr) {&amp;#10;            imageVariable = (imagePath + &quot;Rbluewin.bmp&quot;);&amp;#10;            player_score += trial_r_val;&amp;#10;        } else {&amp;#10;            imageVariable = (imagePath + &quot;Rbluelose.bmp&quot;);&amp;#10;        }&amp;#10;    }&amp;#10;}&amp;#10;if ((player_score &gt; level_limit)) {&amp;#10;    pirate_level += 1;&amp;#10;    leveled_up = true;&amp;#10;    player_score -= level_limit;&amp;#10;}&amp;#10;image.setImage(imageVariable);&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# This code will run at the end of the routine in which it is placed&amp;#10;# Useful for making adjustments to the following routine based on responses in the current routine&amp;#10;&amp;#10;# Ensure that the object you are accessing ('key_resp') is the named exactly the same here as in the builder&amp;#10;if key_resp.keys == 'left': # Start by checking which key was pressed&amp;#10;    if key_resp.corr: # Then see if the response was correct&amp;#10;        imageVariable = imagePath + &quot;Lgreenwin.bmp&quot; # If yes then display the treasure&amp;#10;        player_score += trial_l_val # Add the value displayed to the players score&amp;#10;    else:&amp;#10;        imageVariable = imagePath + &quot;Lgreenlose.bmp&quot; # Otherwise show the empty chest&amp;#10;elif key_resp.keys == 'right': # Same as above but on the other side &amp;#10;    if key_resp.corr:&amp;#10;        imageVariable = imagePath + &quot;Rbluewin.bmp&quot; # The predefined image path is added to the file name&amp;#10;        player_score += trial_r_val # Add the value displayed to the players score&amp;#10;    else:&amp;#10;        imageVariable = imagePath + &quot;Rbluelose.bmp&quot;&amp;#10;&amp;#10;if player_score &gt; level_limit:&amp;#10;    pirate_level += 1&amp;#10;    leveled_up = True&amp;#10;    &amp;#10;    player_score -= level_limit&amp;#10;&amp;#10;&amp;#10;&amp;#10;# Use this to set the image in the following routine&amp;#10;# Again be sure that the object called is named the same thing as the object in the builder&amp;#10;image.setImage(imageVariable)&amp;#10;" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="feedback_code" valType="code"/>
      </CodeComponent>
      <ImageComponent name="background">
        <Param name="color" updates="constant" val="$[1,1,1]" valType="str"/>
        <Param name="colorSpace" updates="constant" val="rgb" valType="str"/>
        <Param name="contrast" updates="constant" val="1" valType="num"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
